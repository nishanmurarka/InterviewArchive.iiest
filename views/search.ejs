<h1 class="pb-4">Search Results</h1>
<div class="row row-cols-2 row-cols-lg-5 g-2 g-lg-3 mb-4">
  <% if(typeof company!='undefined' && company.length > 0) { %>
    <% company.forEach(function(category,index) { %>
      <a href="/company/<%- category.name %>" class="col text-center category__link">
      <div class="category__img shadow">
        <img src= "img/<%- category.image %>" alt="<%- category.name %>" loading="lazy">
      </div>
      <div class="pt-1"><%- category.name %> </div>
    </a>
  <%}) %>

  <% } else { %>
    <p>No items found.</p>
  <% } %>
</div>